{
    "version": "2.51.0",
    // interface language (de, en)
    "language": "de",
    // screen resolution (720p, 1080p)
    "resolution": "720p",
    // target resolution for apps if available (720, 1080, 2160)
    "appTargetResolution": 1080,
    // on off player
    "playMockVideo": true,
    // defines the current environment (tu, prod, preprod)
    "env": "tu",
    // diagnostic app configuration
    "diagnostic": "import({ return './config/diagnostic.' + config.env + '.config.json'; })",
    // logging configuration
    "logging": "import({ return './config/log.' + config.env + '.config.json'; })",
    // error configuration
    "error": "import({ return './config/error.' + config.env + '.config.json'; })",
    // application features
    "features": "import({ return './config/feature.' + config.env + '.config.json'; })",
    // application timings
    "timings": "import(./config/timings.config.json)",
    // radio configuration
    "radio": "import({ return './config/radio.' + config.env + '.config.json'; })",
    // vod configuration
    "vod": "import({ return './config/vod.' + config.env + '.config.json'; })",
    // moretv configuration
    "moretv": "import({ return './config/moretv.' + config.env + '.config.json'; })",
    // uac configuration
    "uac": "import({ return './config/uac.' + config.env + '.config.json'; })",
    // sample for channelinput configuration
    "channelinput": "import(./config/channelinput.config.json)",
    // browse bar configuration
    "browsebar": "import(./config/browsebar.config.json)",
    // settings pages configuration
    "settings": "import(./config/settings.config.json)",
    // timeshift configuration
    "timeshift": "import(./config/timeshift.config.json)",
    // search configuration
    "search": "import(./config/search.config.json)",
    // zenterio configuration
    "zenterio": "import(./config/zenterio.config.json)",
    // load build information
    "buildinfo": "import(./config/build.info.json)",
    // imagescale configuration
    "imagescale": "import({ return './config/imagescale.' + config.env + '.config.json'; })",
    // sam3 authentication configuration (mockup is also using these values)
    "sam3": "import({ return './config/sam3.' + config.env + '.config.json'; })",
    // software update configuration
    "swupdateinfo": "import({ return './config/swupdate.' + config.env + '.config.json'; })",
    // keymap (auto, stb, pc)
    "keymap": "auto",
    // vas items configuration
    "vas": "import(./config/vas.config.json)",
    // youtube configuration
    "youtube": "import({ return './config/youtube.' + config.env + '.config.json'; })",
    // usage tracking
    "usagetracker": "import({ return './config/usagetracker.' + config.env + '.config.json'; })",
    // sat (sat setup)
    "sat": "import({ return './config/sat.' + config.env + '.config.json'; })",
    // editorial (env specific editorial settings)
    "editorial": "import({ return './config/editorial.' + config.env + '.config.json'; })",
    // netflix specific settings
    "netflix": "import({ return './config/netflix.' + config.env + '.config.json'; })",
    // fileblob configuration
    "fileblob": "import(./config/fileblob.config.json)"
}