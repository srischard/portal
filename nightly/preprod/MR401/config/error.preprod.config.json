    "configurations": [
        // --> The following errors are handled by the user interface.
        //
        // ZacMediaPlayerError - 0x20376401\.(21|22)\.[0-9A-F]+
        {
            "errorCode": "^0x20376401\\.(21|22)\\.[0-9A-F]+$",
            "suppressed": [
                "general",
                "report"
            ]
        },
        // ZacMediaPlayerError - 0x20376401\.6\.410+
        {
            "errorCode": "^0x20376401\\.6\\.410+$",
            "suppressed": [
                "general",
                "report"
            ]
        },
        // ZacMediaPlayerError - 0x20376401\.6\.[0-9A-F]+
        {
            "errorCode": "^0x20376401\\.6\\.[0-9A-F]+$"
        },
        // AppVodError - 0x703812(02|03|04|05|06|08)+
        // VodError - 0x40281207\.[0-9A-F]+
        {
            "errorCode": "^0x703812(02|03|04|05|06|08)+|0x40281207\\.[0-9A-F]+$"
        },
        // DialogAbortError - 0x00B[0-9A-F]+
        // OptionPanelServiceError - 0x602[0-9A-F]+
        // UarDialogAbortError - 0x621[0-9A-F]+
        {
            "errorCode": "^0x00B[0-9A-F]+|0x602[0-9A-F]+|0x621[0-9A-F]+$",
            "suppressed": [
                "general",
                "report"
            ]
